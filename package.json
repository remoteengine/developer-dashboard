{
  "name": "developer-dashboard",
  "version": "1.0.0",
  "description": "This is developer dashboard repository",
  "main": "server.js",
  "scripts": {
    "start": "node -r dotenv/config server.js",
    "dev": "dotenv_config_path=.env nodemon -r dotenv/config server.js",
    "lcl": "dotenv_config_path=.env.local nodemon -r dotenv/config server.js",
    "prd": "dotenv_config_path=.env.prod nodemon -r dotenv/config server.js",
    "test": "jest --verbose --detectOpenHandles",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage --verbose",
    "test:integration": "jest tests/integration --verbose",
    "test:unit": "jest tests/unit --verbose",
    "test:file-upload": "jest tests/integration/userFileUpload.test.js tests/unit/userFileUploadService.test.js --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "health": "node healthcheck.js"
  },
  "author": "Amit Kumar",
  "license": "MIT",
  "dependencies": {
    "@bull-board/express": "^6.11.0",
    "@bull-board/ui": "^6.11.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "googleapis": "^150.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "mongoose": "^8.16.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-cron": "^4.2.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "pm2": "^5.3.0",
    "puppeteer": "^24.15.0",
    "redis": "^4.7.1",
    "rotating-file-stream": "^3.2.1",
    "winston": "^3.11.0",
    "zod": "^3.25.74"
  },
  "_moduleAliases": {
    "@root": ".",
    "@config": "src/config",
    "@controllers": "src/controllers",
    "@models": "src/models",
    "@routes": "src/api/v1/routes",
    "@utils": "src/utils"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3"
  }
}
